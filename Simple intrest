#include<stdio.h>
int main()
{
	float principle,time,rate,SI;
	printf("Enter the principle : ");
	scanf("%f",&principle);
	printf("Enter the time: ");
	scanf("%f",&time);
	printf("Enter the rate: ");
	scanf("%f",&rate);
	
	SI=(principle*rate*time/100);
	printf("Simple intrest= %f",SI);	
	return 0;
}
